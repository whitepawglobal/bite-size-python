    "### user dict to build class\n",
    "from pydantic import BaseModel\n",
    "\n",
    "inputvalue = dict(a = 123, b = 456, c = \"temp\", d = True)\n",
    "\n",
    "class DictClass(BaseModel):\n",
    "    \n",
    "    a : int\n",
    "    b : int\n",
    "    c : str\n",
    "    d : bool\n",
    "    \n",
    "    \n",
    "DictClass(**inputvalue)"
   ]
