   "source": [
    "### Given a key value pair, build the reverse key value pair\n",
    "\n",
    "from\n",
    "a = 1\n",
    "b = 2\n",
    "c = 3\n",
    "\n",
    "\n",
    "to \n",
    "1 = a\n",
    "2 = b \n",
    "3 = c\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "id": "a17ee2af-7966-419b-96b4-ebc75d96e5d8",
   "metadata": {
    "tags": []
   },
   "outputs": [
    {
     "data": {
      "text/plain": [
       "{'ABC': 'abc', 'GHJ': 'ghj'}"
      ]
     },
     "execution_count": 1,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "a = dict(abc = \"ABC\", ghj = \"GHJ\")\n",
    "\n",
    "reverse_a = dict(zip(a.values(), a.keys()))\n",
    "\n",
    "reverse_a"
   ]
  },
