   "source": [
    "import re\n",
    "\n",
    "my_str = \"    hello world.   abc def.  ffff f f  f\"\n",
    "_RE_COMBINE_WHITESPACE = re.compile(r\"\\s+\")\n",
    "\n",
    "my_str = _RE_COMBINE_WHITESPACE.sub(\" \", my_str).strip()\n",
    "\n",
    "\n",
    "my_str"
   ]
